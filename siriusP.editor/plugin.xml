<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated My -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="s.presentation.SModelWizardID"
            name="%_UI_SModelWizard_label"
            class="s.presentation.SModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/SModelFile.gif">
         <description>%_UI_SModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated My -->
      <editor
            id="s.presentation.SEditorID"
            name="%_UI_SEditor_label"
            icon="icons/full/obj16/SModelFile.gif"
            extensions="s"
            class="s.presentation.SEditor"
            contributorClass="s.presentation.SActionBarContributor">
      </editor>
   </extension>

</plugin>
